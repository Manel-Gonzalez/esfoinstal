{
    "version": 2,
    "builds": [
        {
            "src": "backend/api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/api/$1.js"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}